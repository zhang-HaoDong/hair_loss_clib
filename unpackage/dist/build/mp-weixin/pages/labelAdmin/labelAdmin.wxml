<view class="label-container"><view class="label-box"><view class="label-header"><view class="label-title">我的标签</view><view data-event-opts="{{[['tap',[['startEdit',['$event']]]]]}}" class="label-edit" bindtap="__e">{{this.isEdit?'完成':'编辑'}}</view></view><view class="label-content"><block wx:for="{{selfTabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="label-content-item">{{''+item.name+''}}<block wx:if="{{isEdit}}"><uni-icons class="icon-close" vue-id="{{'0c5e6160-1-'+index}}" type="clear" size="20" color="red" data-event-opts="{{[['^click',[['deleteSelfTabList',['$0'],[[['selfTabList','',index]]]]]]]}}" bind:click="__e" bind:__l="__l"></uni-icons></block></view></block><block wx:if="{{!selfTabList.length}}"><view class="no-data">当前没有数据</view></block></view></view><view class="label-box"><view class="label-header"><view class="label-title">标签推荐</view></view><view class="label-content"><block wx:for="{{recommendTabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeSelfTabList',['$0'],[[['recommendTabList','',index]]]]]]]}}" class="label-content-item" bindtap="__e">{{''+item.name+''}}</view></block><block wx:if="{{!recommendTabList.length}}"><view class="no-data">当前没有数据</view></block></view></view></view>